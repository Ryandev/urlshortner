{
    "name": "urlshortner",
    "version": "0.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "setup": "yarn setup-install && yarn setup-hooks",
        "setup-install": "yarn install --non-interactive --frozen-lockfile",
        "setup-hooks": "node ./node_modules/husky/lib/bin.js install",
        "uninstall": "rm -rf node_modules",
        "start": "yarn run nx serve frontend",
        "build": "yarn run nx run-many --all --target=build",
        "build-storybook": "yarn run nx run-many --all --target=build-storybook",
        "package": "yarn run nx run-many --all --target=package --parallel=1",
        "test": "yarn run nx run-many --all --target=test",
        "e2e": "yarn run nx run-many --all --target=e2e --parallel=false",
        "lighthouse": "yarn run nx run-many --all --target=lighthouse --parallel=false",
        "lint": "yarn run nx run-many --all --target=lint",
        "pretty": "prettier --config .prettierrc --write 'packages/'",
        "check-pretty": "prettier --check --config .prettierrc 'packages/'",
        "check-spell": "yarn run cspell lint --config 'cspell.json' '**/*.ts'",
        "check-licensing": "fossa analyze && fossa test",
        "check-audit": "yarn run improved-yarn-audit --ignore-dev-deps",
        "clear-cache": "yarn cache clean && rm -rf packages/frontend/.next && rm -rf node_modules/.cache/*",
        "upgrade-nx": "yarn run nx migrate latest --verbose && yarn run nx migrate --run-migrations"
    },
